pipeline {
    agent any 
            environment {
            STAGE_TETS = 'test env'
        }

        stages {
            stage ('BUILD') {
            steps {
                sh 'sleep 5'
            }
            stage ('DEPLOY') {
            steps {
                sh '''
                   #!/bin/bash
                   ls -lrt
                   'sleep 5'
                   '''
            }
            }
        }
    }
}